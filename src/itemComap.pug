doctype html
html(lang='pt')
  style(role='Apenas para navegacao do prototipo, corresponde ao menu-item').
    #btn-navbar--calendar{
      display:none;
    }
    #menu-item--status li li:nth-child(1), #menu-item--status li li:nth-child(3) {
      display:none;
    }
    #menu-item--apoio li li:nth-child(1), #menu-item--apoio li li:nth-child(2), #menu-item--apoio li li:nth-child(3) {
      display:none;
    }
    #menu-item--controle li li:nth-child(1), #menu-item--controle li:nth-child(3), #menu-item--controle li:nth-child(4), #menu-item--controle li:nth-child(5), #menu-item--controle li:nth-child(7), #menu-item--controle li:nth-child(8) {
      display:none;
    }
  - var rand = function () { return Math.random().toString(36).replace(/[^a-z]+/g, '').substr(0, 5) }
