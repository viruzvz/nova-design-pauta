        i.fa.fa-users
      .ArrangeFlex-sizeFill.Box--y.Box--lg
        h2.milli.s-bar--text
          strong Agência
      .ArrangeFlex-sizeFit
        ul.nav.navbar-nav
          li#btn-navbar--link.dropdown.text-center.hidden-xs
            a.navbar-action.zeta.dropdown-toggle(href='#', data-toggle='dropdown')
              span.seta-dropdown-menu.hidden-xs 
              i.fa.fa-link
            ul.dropdown-menu.dropdown-menu--bar.dropdown-menu--lg.pull-right
              li.Box.Box--lg.eta(class="+white +senado")
