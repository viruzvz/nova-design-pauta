                strong  Links
              li.Box.Box--lg.scroll
                ul.list-unstyled.CutHere
                  mixin li
                    li: a(href='#') 
                      i.fa.fa-chevron-right
                      |  Lorem ipsum dolor sit amet, consectetur
                  +li
                  +li
                  +li
                  +li
          li#btn-navbar--calendar.dropdown.text-center.hidden-xs
            a.navbar-action.zeta.dropdown-toggle(href='#', data-toggle='dropdown')
              span.seta-dropdown-menu.hidden-xs
              i.fa.fa-calendar
            ul.dropdown-menu.dropdown-menu--bar.dropdown-menu--lg.pull-right
              li(class="+white")
                .Box.Box--lg
                  .datepicker(data-date='12/03/2012')
