.panel.panel-default
  .s-bar.s-bar--background
    .ArrangeFlex
      .ArrangeFlex-sizeFit.Box.Box--lg
        i.fa.fa-television
      .ArrangeFlex-sizeFill.Box--y.Box--lg
        h2.milli.s-bar--text
          strong TV
      .ArrangeFlex-sizeFit.Box--y.Box--lg.s-bar--columnLeft
        a.toggle.eta.btn-sBar(href='#collapse-box' data-toggle="collapse" aria-expanded='true')
          i.fa.fa-chevron-up
          i.fa.fa-chevron-down
  .panel-body#collapse-box.in
    include ../partials/navbar-nav--secundaria

    // Responsive Select, on only in xs
    .visible-xs
      .form-group
        label(for='mySelect') Selecione um tipo
        select.mySelect.select-nav.form-control
          mixin option(value, name)
            option(value=value) #{name}
          +option('#pautadia', 'Pautas do dia')
          +option('#pautafutura', 'Pautas futuras')
          +option('#pautasfrias', 'Pautas frias')
          +option('#sonoraspendentes', 'Sonoras pendentes')
          +option('#programas', 'Programas')
    // Nav tabs
    ul.nav.nav-tabs.hidden-xs(role='tablist')
      li.active(role='presentation')
        a(href='#pautadia', aria-controls='pautadia', role='tab', data-toggle='tab') Pautas do dia
      li(role='presentation')
        a(href='#pautafutura', aria-controls='pautafutura', role='tab', data-toggle='tab') Pautas futuras
      li(role='presentation')
        a(href='#pautasfrias', aria-controls='pautasfrias', role='tab', data-toggle='tab') Pautas Frias
      li(role='presentation')
        a(href='#sonoraspendentes', aria-controls='sonoraspendentes', role='tab', data-toggle='tab') Sonoras Pendentes
      li(role='presentation')
        a(href='#programas', aria-controls='programas', role='tab', data-toggle='tab') Programas
    // Tab panes
    .tab-content
      #pautadia.tab-pane.active(role='tabpanel')
        .row
          .col-md-4
            .Box.Box--lg.Box--y
              form.form-group
                .input-group.date(data-provide='datepicker', data-date-format="dd/mm/yyyy")
                  input#dataPublicacao.form-control(type='text', placeholder='dd/mm/aaaa')
                  .input-group-addon
                    span.fa.fa-calendar

          .col-md-8.text-right
            mixin btn-impress(title, icon, name)
              button.btn.btn-xs.btn-default(type='button', title=title, data-toggle='tooltip')
                i.fa(class=icon)
                |  #{name}
            .Box.Box--lg.Box--y
              form.form-group
                ul.list-unstyled.list-inline
                  li: +btn-impress('Imprimir tudo', 'fa-print', 'Tudo')
                  li: +btn-impress('Imprimir manhã', 'fa-print', 'Manhã')
                  li: +btn-impress('Imprimir tarde', 'fa-print', 'Tarde')

        mixin thead(head1, head2, head3, head4, head5, head6, head7, head8)
          thead
            tr
              th
                = head1
              th
                = head2
              th
                = head3
              th
                = head4
              th
                = head5
              th
                = head6
        mixin tbody(cond1, cond2, cond3, cond4, cond5, cond6, cond7, cond8)
          tbody
            tr
              td(data-th='Hora')
                i.fa.fa-clock-o
                |  #{cond1}
              td(data-th='Editoria')
                = cond2
              td(data-th='Assunto')
                = cond3
              td(data-th='Equipe Jornalismo')
                .fa.fa-camera
                = cond4
              td(data-th='Equipe Operações')
                = cond5
              td(data-th='Status')
                = cond6


        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Senado', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')

        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Presidência', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')

        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Senado', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')

        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Presidência', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')

      #pautafutura.tab-pane(role='tabpanel')
        .row
          .col-md-4
            .Box.Box--lg.Box--y
              form.form-group
                .input-group.date(data-provide='datepicker', data-date-format="dd/mm/yyyy")
                  input#dataPublicacao.form-control(type='text', placeholder='dd/mm/aaaa')
                  .input-group-addon
                    span.fa.fa-calendar

          .col-md-8.text-right
            mixin btn-impress(title, icon, name)
              button.btn.btn-xs.btn-default(type='button', title=title, data-toggle='tooltip')
                i.fa(class=icon)
                |  #{name}
            .Box.Box--lg.Box--y
              form.form-group
                .btn-block
                  +btn-impress('Imprimir', 'fa-print', 'Imprimir')

        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Senado', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')

      #pautasfrias.tab-pane(role='tabpanel')

      #sonoraspendentes.tab-pane(role='tabpanel')
        .Box--y.Box
          .alert.alert-info
            a(href="#"): strong VT AUDIÊNCIA DE CUSTÓDIA
            ul.list-unstyled
              li Mídia 24391: sonora com o senador Randolfe Rodrigues.
              li Mídia 24121: sonora com o senador Antônio Carlos Valadares.
        .Box--y.Box
          a(href="#"): strong VT ABUSO DE AUTORIDADE
          ul.list-unstyled
            li Mídia 24121: sonora com os senadores Ana Amélia e Randolfe Rodrigues.
            li Mídia 24222- Sonora com o senador Garibaldi
            li Mídia 25709 - Sonora com os senadores Telmário Mota, Vanessa Graziottin e Magno Malta.
        .Box--y.Box
          .alert.alert-info
            a(href="#"): strong VT JOGOS DE AZAR
            ul.list-unstyled
              li Fita 49526: sonora com senadores Sérgio Petecão e Antonio Anastasia sobre jogos de azar.
              li Mídia 24465: sonora com a sen. Gleisi Hoffmann sobre jogos de azar (é contra).
        .Box--y.Box
          a(href="#"): strong ESPECIAL PREVIDÊNCIA
          ul.list-unstyled
            li Mídia 24357: Aloysio Nunes, Fátima Bezerra, Benedito Lira, José Pimentel, Ataídes Oliveira, Antonio Carlos Valadares, Alvaro Dias;
            li Mídia 24299: Humberto Costa, Lídice da Mata, Paulo Paim;
            li Mídia 24382: Cidinho Santos.
      #programas.tab-pane(role='tabpanel')
        form.form-inline
          .row
            .col-md-8
              .Box.Box--lg.Box--y
                .form-group
                  label.control-label.milli(for='quantidade') Programação:
                  select.form-control
                    option Todos
                    option Abertura dos Trabalhos 2016
                    option Abertura dos Trabalhos 2015
                    option Agenda Econômica
                    option Alô Senado
                    option Argumento
                    option Cidadania
                    option Diplomacia
                    option Ecosenado
                    option Eleições 2014
                    option Em Discussão
                    option Inclusão
                    option Município Brasil
                    option Parlamento
                    option Posse Presidencial
                    option Prisma
                    option Repórter Senado
                    option Revista da Semana

            .col-md-4.text-right
              mixin btn-impress(title, icon, name)
                button.btn.btn-xs.btn-success(type='button', title=title, data-toggle='tooltip')
                  i.fa(class=icon)
                  |  #{name}
              .Box.Box--lg.Box--y
                form.form-group
                  .btn-block
                    +btn-impress('Nova Pauta Programa', 'fa-plus', 'Nova Pauta Programa')

        table.table.table-striped.table-hover.table-rp.milli
          +thead('Hora', 'Editoria', 'Assunto', 'Equipe Jornalismo', 'Equipe Operações', 'Status')
          +tbody('08:00', 'Senado', 'ORIENTAÇÕES PARA COBERTURA DO DIA 9/11 - QUARTA-FEIRA ', 'Ricardo Ribeiro', 'Ilário Sobrinho', 'Privado')
